<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
	  <section name="PublicationConfigSection"
			   type="AppPublication.Config.Publication.PublicationConfigSection, AppPublication" />
	  <section name="GenerationConfigSection"
			   type="AppPublication.Config.Generation.GenerationConfigSection, AppPublication" />
  </configSections>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        throwConfigExceptions="true">
    <variable name="loggingLevel" value="Info" />
    <targets>
		<target name="logFile" xsi:type="File" fileName="./Log/AppPublication.log" maxArchiveFiles="50" archiveAboveSize="10485760" archiveNumbering="Rolling" layout="${when:when=level&lt;=LogLevel.Info:inner=${longdate}|${level}|${callsite:className=True:fileName=False:includeSourcePath=False:methodName=True:captureStackTrace=False}|${message} |${all-event-properties} ${exception:format=toString}:else=${longdate}|${level}|${callsite:className=True:fileName=False:includeSourcePath=False:methodName=True:captureStackTrace=False}|${message} |${all-event-properties} ${exception:format=toString}${stacktrace:format=DetailedFlat:topFrames=20:separator=&#xD;&#xA;}}" />
		<target name="logDataFile" xsi:type="File" fileName="./Log/AppPublication_DebugData.log" maxArchiveFiles="50" archiveAboveSize="104857600" archiveNumbering="Rolling"/>
	</targets>
    <rules>
        <logger name="defaultLogger" minlevel="${var:loggingLevel}" writeTo="logfile" />
        <logger name ="dbgDataLogger" minlevel="${var:loggingLevel}" writeTo="logDataFile" />
	</rules>
  </nlog>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
	<!-- Utilisation de la section pour les paramÃ¨tres globaux -->
	<PublicationConfigSection>
		<general repertoireRacine="C:\PublicationSite" logo="logo-France-Judo-Rhone.png" easyConfig="True" urlDistant="http://adresse-du-site" isolerCompetition="True" repertoireRacineSiteFTPDistant="/" effacerAuDemarrage="True" />
		<schedulers>
			<scheduler id="public" delaiGenerationSec="30"/>
			<scheduler id="internal" delaiGenerationSec="30"/>
		</schedulers>
		<miniSites>
			<miniSite id="local" interface="192.168.0.1" />
			<miniSite id="distant" ftpLogin=""  ftpPassword="" ftpSite="" modeActif="false" syncDiff="true"/>
			<miniSite id="ffjudo" ftpPassword=""/>
		</miniSites>
	</PublicationConfigSection>

	<GenerationConfigSection>
		<generateurSite
			delaiActualisationClientSec="20"
			tailleMaxPouleColonnes="6"
			pouleEnColonnes="False"
			pouleToujoursEnColonnes="False"
			publierProchainsCombats="True"
			nbProchainsCombats="6"
			msgProchainsCombats=""
			publierAffectationTapis="True"/>
		<ecrans>
			<ecran id="1" description="ecran1" adresseIp="192.168.0.1" hostname="monPc"/>
		</ecrans>
	</GenerationConfigSection>
	<system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>